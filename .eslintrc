{
  "extends": "eslint",
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "rules": {
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
     "indent":[
      0
     ],
     "no-trailing-spaces":1,
     "func-style": [
      0,
      "declaration"
    ],
     "new-cap": [
      0
    ]
  }
}
